<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="AltaCopia" prefHeight="314.0" prefWidth="565.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lblMedio" layoutX="35.0" layoutY="31.0" text="Medio a Copiar:" />
      <Label fx:id="lblFormato" layoutX="35.0" layoutY="69.0" text="Formato:" />
      <TextField fx:id="txtMedio" disable="true" layoutX="140.0" layoutY="31.0" prefHeight="25.0" prefWidth="230.0" />
      <TextArea fx:id="txtDescripcion" layoutX="35.0" layoutY="152.0" prefHeight="82.0" prefWidth="506.0" promptText="Escriba una corta descripción" />
      <Button fx:id="btnFinalizar" layoutX="140.0" layoutY="253.0" mnemonicParsing="false" onAction="#agregarCopia" prefHeight="25.0" prefWidth="106.0" text="Crear Copia" visible="false" />
      <Button fx:id="btnEliminar" layoutX="140.0" layoutY="253.0" mnemonicParsing="false" onAction="#cancelar" prefHeight="25.0" prefWidth="106.0" text="Eliminar Copia" visible="false" />
      <ComboBox fx:id="cmbFormato" accessibleText="Seleccion de sistema operativo" layoutX="140.0" layoutY="69.0" onAction="#ComboBoxActivo" prefHeight="25.0" prefWidth="230.0" promptText="Seleccionar..." />
      <Label fx:id="lblUbicaciones" layoutX="35.0" layoutY="114.0" text="Ubicación:" />
      <ComboBox fx:id="cmbUbicaciones" layoutX="140.0" layoutY="110.0" prefHeight="25.0" prefWidth="230.0" promptText="Seleccionar..." />
      <CheckBox fx:id="chkDeposito" layoutX="401.0" layoutY="114.0" mnemonicParsing="false" text="En Depósito" />
      <Button fx:id="btnModificar" layoutX="140.0" layoutY="253.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" text="Modificar Copia" visible="false" />
      <Button fx:id="btnCancelar" layoutX="274.0" layoutY="253.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" text="Cancelar" />
   </children>
</AnchorPane>
